describe "Group mijo"{

	test "Mijo mojarse" {
	  mijo.mojarse()
	  pepita.comer(mijo, 10)
		assert.equals(150, pepita.energia())
	}
	
	test "Mijo secarse" {
	  pepita.secarse()
	  ave.comer(mijo, 10)
	  assert.equals(200, pepita.energia())
	}
	
	test "Mijo mojarse y secarse" {
	  mijo.mojarse()
	  pepita.comer(mijo, 10)
	  pepita.secarse()
	  ave.comer(mijo, 10)
	  assert.equals(350, pepita.energia())
	}
	
}